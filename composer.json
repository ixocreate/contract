{
    "name": "ixocreate/contract",
    "description": "The IXOCREATE contract package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "ixocreate/application-contract": "dev-develop",
        "ixocreate/servicemanager-contract": "dev-develop",
        "ixocreate/event-contract": "dev-develop",
        "ixocreate/filter-contract": "dev-develop",
        "ixocreate/command-bus-contract": "dev-develop",
        "ixocreate/schema-contract": "dev-develop",
        "ixocreate/type-contract": "dev-develop",
        "ixocreate/resource-contract": "dev-develop",
        "ixocreate/admin-contract": "dev-develop"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Contract\\Command\\": "src/Command/",
            "Ixocreate\\Contract\\Entity\\": "src/Entity/",
            "Ixocreate\\Contract\\Http\\": "src/Http/",
            "Ixocreate\\Contract\\Template\\": "src/Template/",
            "Ixocreate\\Contract\\Validation\\": "src/Validation/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
