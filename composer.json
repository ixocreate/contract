{
    "name": "ixocreate/contract",
    "description": "The IXOCREATE contract package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "ixocreate/servicemanager-contract": "^1.0.0-alpha1",
        "ixocreate/admin-contract": "^1.0.0-alpha1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Contract\\Application\\": "src/Application/",
            "Ixocreate\\Contract\\Command\\": "src/Command/",
            "Ixocreate\\Contract\\CommandBus\\": "src/CommandBus/",
            "Ixocreate\\Contract\\Entity\\": "src/Entity/",
            "Ixocreate\\Contract\\Event\\": "src/Event/",
            "Ixocreate\\Contract\\Filter\\": "src/Filter/",
            "Ixocreate\\Contract\\Http\\": "src/Http/",
            "Ixocreate\\Contract\\Registry\\": "src/Registry/",
            "Ixocreate\\Contract\\Resource\\": "src/Resource/",
            "Ixocreate\\Contract\\Schema\\": "src/Schema/",
            "Ixocreate\\Contract\\Template\\": "src/Template/",
            "Ixocreate\\Contract\\Type\\": "src/Type/",
            "Ixocreate\\Contract\\Validation\\": "src/Validation/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
