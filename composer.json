        {
            "name": "IXOCREATE"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
